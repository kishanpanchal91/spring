<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
		  "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="t" class="beans.Test">
		<property name="cars">
			<list>
				<value>Audi</value>
				<value>Honda</value>
				<value>Maruti</value>
				<value>Hyundai</value>
			</list>
		</property>
			<property name="model">
			<set>
				<value>2012</value>
				<value>2013</value>
				<value>2014</value>
				<value>2015</value>
			</set>
		</property>
		<property name="engine">
		<map>
		<entry key="VTVT" value="Petrol"/>
		<entry key="CRDI" value="Disel"/>
	
		
		
		</map>
		</property>

	</bean>
</beans>